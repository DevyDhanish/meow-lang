# simple sorting algo in meowlang


arr = {23,1,3,53,26,8,9,0}

func sort(array)
{
    i = 0
    size = len(array)

    while i < size
    {
        j = 0
        while j < size
        {
            if array[i] < array[j]
            {
                temp = array[i]
                array[i] = array[j]
                array[j] = temp
            }
            j += 1
        }
        i += 1
    }
}

sort(arr)

print("Sorted array -> ", arr)