func name( name, position )
{
    return name[position]
}

print(name("Dhanish", 4))

func check_if_correct( name,position,actual)
{
    if name[position] == actual
    {
        return true
    }

    else
    {
        return false
    }
}

if check_if_correct("Dhanish", 1, "h") {
    print("Yes")
}
else
{
    print("No")
}