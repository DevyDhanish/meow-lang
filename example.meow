@add "meowio"       // A simple lib for input and output "meowio.meow"

x = 100;

show(x)

if x > 5 {
    show("x is greater than 5")
}
else{
    show("x is less than 5")
}

while x < 5{
    show(x)
    x -= 1
}

return 0